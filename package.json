{
  "name": "janes-school",
  "packageManager": "bun@1.1.29",
  "type": "module",
  "private": true,
  "keywords": [
    "template",
    "biomejs",
    "bun",
    "typescript",
    "tailwindcss",
    "astrojs",
    "cloudflare",
    "react",
    "framer-motion",
    "shadcn"
  ],
  "author": {
    "email": "devopsick@pm.me",
    "name": "Hrihorii Ilin - @Frikadellios",
    "url": "https://github.com/Frikadellios"
  },
  "license": "MIT",
  "description": "I am from Ukraine :D",
  "repository": {
    "type": "git",
    "url": "https://github.com/Frikadellios/janes-school.git"
  },
  "bugs": {
    "url": "https://github.com/Frikadellios/janes-school/issues"
  },
  "homepage": "https://github.com/Frikadellios/janes-school/readme",
  "engines": {
    "node": ">=18.19.1 <19 || >=20.17.0 <21 || >=21.1.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-checker -exclude \"**/node_modules/**\"",
    "browserslist": "bunx update-browserslist-db@latest",
    "dev": "bunx --bun astro dev --verbose",
    "start": "bun wrangler types && bun astro check --verbose && bunx --bun astro dev --verbose",
    "build": "bun wrangler types && bun astro check --verbose && bunx --bun astro build --verbose",
    "preview": "bunx --bun astro build && bun wrangler pages dev ./dist",
    "astro": "astro",
    "deploy": "bunx --bun astro build && bun wrangler pages deploy ./dist",
    "cf-typegen": "wrangler types",
    "tailwindcss": "bunx @tailwindcss/cli@next -i ./src/styles/app.css -o ./src/styles/dist/app.css",
    "lint": "conc bun:lint:*",
    "lint:format": "bunx @biomejs/biome format . --write",
    "lint:fix": "bunx @biomejs/biome check . --fix",
    "lint:check": "bunx @biomejs/biome check ./* --write --unsafe",
    "typesafe-i18n": "typesafe-i18n"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/cloudflare": "^11.1.0",
    "@astrojs/mdx": "^3.1.7",
    "@astrojs/partytown": "^2.1.2",
    "@astrojs/react": "^3.6.2",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/ts-plugin": "^1.10.2",
    "@csstools/postcss-oklab-function": "^4.0.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@tailwindcss/cli": "^4.0.0-alpha.25",
    "@tailwindcss/node": "^4.0.0-alpha.21",
    "@tailwindcss/postcss": "^4.0.0-alpha.25",
    "@tailwindcss/vite": "^4.0.0-alpha.25",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.15.10",
    "astro-robots-txt": "^1.0.0",
    "astro-zod-to-json-schema": "^0.0.2",
    "balance-text": "^3.3.1",
    "caniuse-lite": "^1.0.30001666",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^9.0.1",
    "date-fns": "^4.1.0",
    "detect-browser-es": "^0.1.1",
    "framer-motion": "^11.9.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.447.0",
    "next-themes": "^0.3.0",
    "postcss-discard-comments": "^7.0.3",
    "postcss-preset-env": "^10.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "reading-time": "^1.5.0",
    "rehype-external-links": "^3.0.0",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^4.0.0-alpha.25",
    "typesafe-i18n": "^5.26.2",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@cloudflare/workers-types": "^4.20240925.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/bun": "latest",
    "@types/lodash-es": "^4.17.12",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.7.4",
    "@types/web": "^0.0.167",
    "bun-devtools": "latest",
    "bun-types": "latest",
    "editorconfig-checker": "^6.0.0",
    "unplugin-auto-import": "^0.18.3",
    "vite-plugin-handlebars": "^2.0.0",
    "vite-plugin-lightningcss": "^0.0.5",
    "wrangler": "^3.79.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "trustedDependencies": ["@biomejs/biome", "esbuild", "sharp", "workerd"]
}
